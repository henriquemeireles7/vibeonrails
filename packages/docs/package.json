{
  "name": "@vibeonrails/docs",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets/index.js",
      "types": "./dist/presets/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    },
    "./generator": {
      "import": "./dist/generator/index.js",
      "types": "./dist/generator/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "unified": "^11.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "handlebars": "^4.7.0",
    "ts-morph": "^27.0.2"
  },
  "peerDependencies": {
    "@astrojs/starlight": ">=0.30.0",
    "astro": ">=5.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@astrojs/starlight": "^0.32.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^25.2.1",
    "@types/react": "^19.0.0",
    "astro": "^5.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}

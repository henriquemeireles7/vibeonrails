      "types": "./dist/health/index.d.ts"
    },
    "./logging": {
      "import": "./dist/logging/index.js",
      "types": "./dist/logging/index.d.ts"
    },
    "./queue": {
      "import": "./dist/queue/index.js",
      "types": "./dist/queue/index.d.ts"
    },
    "./email": {
      "import": "./dist/email/index.js",
      "types": "./dist/email/index.d.ts"
    },
    "./cache": {
      "import": "./dist/cache/index.js",
      "types": "./dist/cache/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    },
    "./realtime": {
      "import": "./dist/realtime/index.js",
      "types": "./dist/realtime/index.d.ts"
    },
    "./monitoring": {
      "import": "./dist/monitoring/index.js",
      "types": "./dist/monitoring/index.d.ts"
    },
    "./rate-limit": {
      "import": "./dist/rate-limit/index.js",
      "types": "./dist/rate-limit/index.d.ts"
    },
    "./flags": {
      "import": "./dist/flags/index.js",
      "types": "./dist/flags/index.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics/index.js",
      "types": "./dist/analytics/index.d.ts"
    }

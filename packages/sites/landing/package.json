{
  "name": "@vibeonrails/site-landing",
  "version": "0.1.0",
  "type": "module",
  "description": "One-sentence landing page generator for Vibe on Rails",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"

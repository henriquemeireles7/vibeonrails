{
  "name": "@vibeonrails/companion",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenClaw skills that turn any OpenClaw instance into a VoR-aware business operator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "@vibeonrails/core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@vibeonrails/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "skills",
    "personality"
  ]

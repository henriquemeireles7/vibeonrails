{
  "name": "@vibeonrails/web",
  "version": "0.1.0",
  "type": "module",
  "description": "Frontend package for Vibe on Rails â€” CSS system, React components, hooks, tRPC client",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles/index.js",
      "types": "./dist/styles/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./routing": {
      "import": "./dist/routing/index.js",
      "types": "./dist/routing/index.d.ts"
    },
    "./styles/*.css": "./dist/styles/*.css"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zustand": "^5.0.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.36.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jsdom": "^25.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist"
  ]
}
